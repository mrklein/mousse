// mousse: CFD toolbox

FoamFile
{
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      k;
}

dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 1e-10;

boundaryField
{
  lowerWall
  {
    type            kqRWallFunction;
    value           uniform 1e-10;
  }
  upperWall
  {
    type            kqRWallFunction;
    value           uniform 1e-10;
  }
  front
  {
    type            cyclic;
  }
  back
  {
    type            cyclic;
  }
  defaultFaces
  {
    type            empty;
  }
}

// vim: set ft=foam et sw=2 ts=2 sts=2:
